	<queryString language="SQL">
		<![CDATA[select g.name as company, a.prno, e.description as ordertype, '' as rfqno, k.site_description as site
, n.delivery_location as delivery_address
, a.functions, a.department, a.division, isnull(a.requested_for_email, '') as email
, isnull(a.requested_for_phone, '') as phone ,  isnull(a.requested_for_ext,'')  as ext  
, f.description as status, a.request_date, b.name + ' ' + b.lastname as requested_for
, c.name + ' ' + c.lastname as requestor, d.name + ' ' + d.lastname as buyer
, h.partno, h.description, h.qty, h.purchase_unit, h.unit_price, h.qty * h.unit_price as total
, h.final_price as unit_price_final, h.final_price * h.qty as final_price, h.budget_code, h.req_date
, l.name1 + ' ' + l.name2 as nominated_supplier, h.edecision, h.remarks,  j.name + ' ' + j.lastname as approver
,  j.position, i.changed_on as approve_date, h.currency, h.[lineno]
from pr_header a
left join users b on b.id = a.requested_for
left join users c on c.id = a.requestor
left join users d on d.username = a.buyer
left join order_type e on e.ordertype = a.ordertype
left join pr_status f on f.status = a.status
left join company g on g.company = a.company
left join pr_item h on h.prno_id = a.id
left join dec_val_workflow i on i.ref_doc_id = a.id and i.approval_type='DECIDER' and i.status='30'
left join users j on j.username = i.approver
left join (select site, site_description, delivery_location from site group by site, site_description, delivery_location) as k on a.site = k.site
left join supplier l on h.nominated_supplier = l.supplier
left join site n on a.delivery_address=n.address_id
where a.prno='$P!{prno}']]>
	</queryString>
